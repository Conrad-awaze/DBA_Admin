  <DataSets>
    <DataSet Name="MainDataSet">
      <Query>
        <DataSourceName>THG</DataSourceName>
        <CommandText>-- Cheque Run GBP

DECLARE @OwnerTable Table (
	OwnerID int
)

-- Payment code 01-10
INSERT INTO @OwnerTable
SELECT	OwnerID
FROM	thg_OwnerPaymentRunStatementSequence
WHERE	PaySequence &lt;&gt; 0
AND	CurrencyID = 2 --Change to EURO
AND	PaymentMethodID = 5810 
ORDER BY	CurrencyID,
	PaySequence,
	CASE WHEN Amount &gt; 0 THEN 1 ELSE 0 END,	
	CASE WHEN Amount &gt; 5000 THEN 1 ELSE 0 END,
	OwnerID

-- Overseas Owners
INSERT INTO @OwnerTable	
SELECT	OwnerID
FROM	thg_OwnerPaymentRunStatementSequence
WHERE	PaySequence = 0
AND	NonResidentLandlord = 1
AND	CurrencyID = 2 --Change to EURO
AND	PaymentMethodID = 5810
ORDER BY	CurrencyID,
	PaySequence,
	CASE WHEN Amount &gt; 0 THEN 1 ELSE 0 END,	
	CASE WHEN Amount &gt; 5000 THEN 1 ELSE 0 END,
	OwnerID
	
-- Negative Balances

INSERT INTO @OwnerTable
SELECT	OwnerID
FROM	thg_OwnerPaymentRunStatementSequence
WHERE	PaySequence = 0
AND	NonResidentLandlord = 0
AND	Amount &lt; 0
AND	CurrencyID = 2 --Change to EURO
AND	PaymentMethodID = 5810
ORDER BY	OwnerID

-- Payment Cheque
INSERT INTO @OwnerTable
SELECT	OwnerID
FROM	thg_OwnerPaymentRunStatementSequence
WHERE	PaySequence = 0
AND	NonResidentLandlord = 0
AND	Amount &gt;= 0
AND	PaymentMethodID = 5810 
AND	CurrencyID = 2 --Change to EURO
ORDER BY	PaymentMethodID,
	CASE WHEN Amount &gt; 5000 THEN 1 ELSE 0 END,
	OwnerID
	
SELECT	OwnerID
FROM	@OwnerTable 
</CommandText>
      </Query>
