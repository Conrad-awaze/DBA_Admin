  <DataSets>
    <DataSet Name="ReconciliationDetail">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Mode">
            <Value>=Parameters!Mode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierType">
            <Value>=Parameters!SupplierType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StatementRef">
            <Value>=Parameters!StatementRef.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT    CONVERT(CHAR(13), F.Dep, 106) AS 'Dep Date', 
	F.BookingID, 
	F.Ref AS PNR, 
	C.Name, 
	ST.PayAmount, 
	CASE WHEN ISNULL(ST.Notes,'') &lt;&gt; '' THEN ST.Notes + ' - ' + CONVERT(VARCHAR(11), ST.Updated, 106) ELSE '' END  AS Notes  , 
	M.Name + ' - ' + M.Symbol AS Currency
FROM	thg_StatementDetail AS ST 
	INNER JOIN Ferry AS F ON F.ID = ST.Ferry_Id 
	INNER JOIN thg_Statement AS S ON S.Id = ST.Statement_Id 
	INNER JOIN Booking AS B ON B.ID = F.BookingID 
	INNER JOIN Client AS C ON C.ID = B.ClientID 
	LEFT OUTER JOIN Money AS M ON M.ID = S.Currency_Id
WHERE     (S.Supplier_Id = @SupplierID) 
	AND (S.StatementFromDate &lt;= @StartDate) 
	AND (S.StatementToDate &gt;= @EndDate)
	AND (1 = CASE WHEN @Mode = 1 THEN 1 ELSE (CASE WHEN ST.Pay = 1 THEN 1 ELSE 0 END) END)  /* @Mode of 1 means preview mode. Anything else means Processed mode */
	AND (S.SupplierType_Id = @SupplierType)
	AND (S.Reference = @StatementRef)
	AND ST.Flag = 'F' -- Only the items processed in this statement
ORDER BY	F.Dep DESC,
	F.BookingID ASC,
	F.Ref ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="DepDate">
          <DataField>Dep Date</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="BookingID">
          <DataField>BookingID</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="PNR">
          <DataField>PNR</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="PayAmount">
          <DataField>PayAmount</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Notes">
          <DataField>Notes</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Currency">
          <DataField>Currency</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ReconciliationHeader">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierType">
            <Value>=Parameters!SupplierType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Mode">
            <Value>=Parameters!Mode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Ref AS VARCHAR(200)
SET @Ref = ''
SELECT @Ref = @Ref +  S.Reference + ' , '  
FROM thg_StatementDetail ST
INNER JOIN	Ferry F ON F.ID = ST.Ferry_Id
INNER JOIN	thg_Statement S ON S.Id = ST.Statement_Id
WHERE	S.Supplier_Id = @SupplierID
	AND	S.StatementFromDate &lt;= @StartDate AND S.StatementToDate &gt;= @EndDate
	AND	1 = CASE WHEN @Mode = 1 THEN 1 ELSE (CASE WHEN ST.Pay = 1 THEN 1 ELSE 0 END) END
GROUP BY S.Reference



SELECT 
(SELECT Name FROm Supplier WHERE ID = @SupplierID) AS SupName,
 CASE WHEN ISNULL(@Ref, '') = '' THEN '' ELSE SUBSTRING(@Ref,1,LEN(@Ref)-1) END AS SupRef,
(SELECT	Name FROM Lookup WHERE PID = 3 AND ID = @SupplierType) AS SupplierTypeName</CommandText>
      </Query>
      <Fields>
        <Field Name="SupName">
          <DataField>SupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SupRef">
          <DataField>SupRef</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SupplierTypeName">
          <DataField>SupplierTypeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Supplier">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <CommandText>
  CREATE TABLE #Supplier(
  TableID int,        
  ID int,        
  Name VARCHAR(200)        
  )    

INSERT INTO #Supplier
SELECT		ROW_NUMBER() OVER(ORDER BY S.Name ASC),S.ID, S.Name + ' (' + L.Name  + ')'
FROM		Supplier S
INNER JOIN  Lookup L ON S.TID = L.ID
ORDER BY	S.Name ASC

SELECT	ID,Name 
FROM	#Supplier
ORDER BY TableID ASC

DROP TABLE #Supplier</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SupplierType">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <CommandText>
  CREATE TABLE #SupplierType(
  TableID int,        
  ID int,        
  Name VARCHAR(50)        
  ) 

INSERT INTO #SupplierType
SELECT		ROW_NUMBER() OVER(ORDER BY Name ASC),ID, Name 
FROM		Lookup
WHERE		PID = 3
ORDER BY	Name ASC

SELECT ID,Name FROM #SupplierType
ORDER BY TableID ASC

DROP TABLE #SupplierType</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Mode">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <CommandText>DECLARE	@Modes table (	ID int, 
			Name varchar(50)
	)

INSERT	@Modes (	ID, 
		Name
	)
VALUES	(1, 'Preview'),
	(0, 'Processed')

SELECT	*
FROM	@Modes</CommandText>
      </Query>
