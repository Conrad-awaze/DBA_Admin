  <Code>    Function TextEurosSplitIndex(ByVal TextEuros As String, ByVal TextBoxNumber As Integer) As Integer

        Dim textEurosLength As Integer = TextEuros.Length
        Dim TextEurosSubstring As String
        Dim index As Integer = -1
        Dim isSpaceChar As Boolean = False
        Dim spaceIndex As Integer
        Dim dashIndex As Integer

        If (textEurosLength &lt;= 31) Then
            Return -1
        End If

        TextEurosSubstring  = TextEuros.Substring(0, 32)

        While True
            spaceIndex = TextEurosSubstring.IndexOf(" ", index + 1, TextEurosSubstring.Length - index - 1)
            dashIndex = TextEurosSubstring.IndexOf("-", index + 1, TextEurosSubstring.Length - index - 1)
            If spaceIndex &gt; index And spaceIndex &gt; dashIndex Then
                index = spaceIndex
                isSpaceChar = True
            Else
                If dashIndex &gt; index Then
                    index = dashIndex
                    isSpaceChar = False
                Else
                    Exit While
                End If
            End If
        End While

        If TextBoxNumber = 1 And isSpaceChar Then
                Return index
        Else
                Return index + 1
        End If
    End Function

    Function TextEurosFullString(ByVal TextEuros As String, ByVal FractionalUnits As String) As String
        
        Dim ReturnString As String

        If TextEuros.Length &gt; 0 Then
            ReturnString = TextEuros + " EUROS "
        End If

        If FractionalUnits.Length &gt; 0 Then
            ReturnString += FractionalUnits + "c"
        End If

        Return ReturnString

    End Function

    Function TextEuros1Content(ByVal TextEuros As String) As String
        Dim splitIndex As Integer = TextEurosSplitIndex(TextEuros, 1)

        If splitIndex = -1 Then
                Return TextEuros
        Else
                Return TextEuros.Substring(0, splitIndex )
        End If
    End Function

    Function TextEuros2Content(ByVal TextEuros As String) As String
        Dim splitIndex As Integer = TextEurosSplitIndex(TextEuros, 2)

        If splitIndex = -1 Then
                Return ""
        Else
                Return TextEuros.Substring(splitIndex, TextEuros.Length - splitIndex)
        End If
    End Function</Code>
