  <Code>    Function TextEurosFullString(ByVal TextEuros As String, ByVal FractionalUnits As String) As String
        
        Dim ReturnString As String

        If TextEuros.Length &gt; 0 Then
            ReturnString = TextEuros + " EUROS "
        End If

        If FractionalUnits.Length &gt; 0 Then
            ReturnString += FractionalUnits + "c"
        End If

        Return ReturnString

    End Function</Code>
