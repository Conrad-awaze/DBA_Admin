  <DataSets>
    <DataSet Name="ReconciliationDetail">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@SupplierRef">
            <Value>=Parameters!SupplierRef.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	CONVERT(CHAR(13),F.Dep,106) AS 'Dep Date',
	F.BookingID,
	F.Ref AS PNR,
	C.Name, 
	ST.PayAmount,
	CASE WHEN ISNULL(ST.Notes,'') &lt;&gt; '' THEN ST.Notes + ' - ' + CONVERT(VARCHAR(11), ST.Updated, 106) ELSE '' END  AS Notes,
	M.Name + ' - ' + M.Symbol AS Currency,
	B.AccountingGroupCode Accountgroup
FROM	thg_StatementDetail ST
	INNER JOIN Ferry F ON F.ID = ST.Ferry_Id
	INNER JOIN thg_Statement S ON S.Id = ST.Statement_Id
	INNER JOIN Booking B ON B.ID = F.BookingID
	INNER JOIN Client C ON C.ID = B.ClientID
	LEFT OUTER JOIN Money M ON M.ID = S.Currency_Id
WHERE	S.Supplier_Id = @SupplierID
AND	S.Reference = @SupplierRef
AND	1 = CASE WHEN ST.Pay = 1 THEN 1 ELSE 0 END
AND	ST.Flag = 'F' -- Only the items processed in this statement
ORDER BY	F.Dep DESC, 
	F.BookingID ASC, 
	F.Ref ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="DepDate">
          <DataField>Dep Date</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="BookingID">
          <DataField>BookingID</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="PNR">
          <DataField>PNR</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="PayAmount">
          <DataField>PayAmount</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Notes">
          <DataField>Notes</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AccountGroup">
          <DataField>Accountgroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Currency">
          <DataField>Currency</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ReconciliationHeader">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SupplierID">
            <Value>=Parameters!SupplierID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierRef">
            <Value>=Parameters!SupplierRef.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SupplierType">
            <Value>=Parameters!SupplierType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
(SELECT Name FROm Supplier WHERE ID = @SupplierID) AS SupName,
(SELECT CONVERT(CHAR(13),S.StatementFromDate,106) FROM thg_Statement S WHERE S.Reference = @SupplierRef  AND Supplier_Id = @SupplierID) AS StartDate ,
(SELECT CONVERT(CHAR(13),S.StatementToDate,106) FROM thg_Statement S WHERE S.Reference = @SupplierRef AND Supplier_Id = @SupplierID) AS EndDate,
(SELECT	Name FROM Lookup WHERE PID = 3 AND ID = @SupplierType) AS SupplierTypeName</CommandText>
      </Query>
      <Fields>
        <Field Name="SupName">
          <DataField>SupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EndDate">
          <DataField>EndDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SupplierTypeName">
          <DataField>SupplierTypeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Supplier">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <CommandText>
  CREATE TABLE #Supplier(
  TableID int,        
  ID int,        
  Name VARCHAR(200)        
  )    
  
INSERT INTO #Supplier
VALUES(0,-1,'Select Supplier')

INSERT INTO #Supplier
SELECT		ROW_NUMBER() OVER(ORDER BY S.Name ASC),S.ID, S.Name + ' (' + L.Name  + ')'
FROM		Supplier S
INNER JOIN  Lookup L ON S.TID = L.ID
ORDER BY	S.Name ASC

SELECT	ID,Name 
FROM	#Supplier
ORDER BY TableID ASC

DROP TABLE #Supplier</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SupplierType">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <CommandText>
  CREATE TABLE #SupplierType(
  TableID int,        
  ID int,        
  Name VARCHAR(50)        
  )    
  
INSERT INTO #SupplierType
VALUES(0,-1,'Select Supplier Type')

INSERT INTO #SupplierType
SELECT		ROW_NUMBER() OVER(ORDER BY Name ASC),ID, Name 
FROM		Lookup
WHERE		PID = 3
ORDER BY	Name ASC

SELECT ID,Name FROM #SupplierType
ORDER BY TableID ASC

DROP TABLE #SupplierType</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
