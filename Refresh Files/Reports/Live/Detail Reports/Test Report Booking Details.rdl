  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>VRUKL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BookingID">
            <Value>=Parameters!BookingID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	B.ID AS BookingID,
	ISNULL(C.Title + ' ', '') + ISNULL(C.Fore + ' ', '') + ISNULL(C.Name,'') AS ClientName,
	B.Created,
	B.Depart
FROM	Booking B
INNER JOIN Client C ON B.ClientID = C.ID
WHERE	B.ID = @BookingID</CommandText>
      </Query>
      <Fields>
        <Field Name="BookingID">
          <DataField>BookingID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ClientName">
          <DataField>ClientName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Created">
          <DataField>Created</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Depart">
          <DataField>Depart</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
